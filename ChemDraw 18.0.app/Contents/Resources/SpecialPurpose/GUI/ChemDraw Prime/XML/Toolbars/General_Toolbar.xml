<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar columns="15" rows="1" columnsVertical="1" rowsVertical="13" preferenceKey="General"
        defaultOrientation="horizontal"
        hideOnApplicationSwitch="true" uniqueID="GeneralToolbarID_Ext" defaultDockedLocation="topLeft">
        <Title localizationName="GeneralToolbarTitle">General</Title>
        <TextMenuItem>
            <Title localizationName="GeneralToolbarMenu">Show General Toolbar</Title>
        </TextMenuItem>
        <Tooltip localizationName="GeneralTooltip">General Commands</Tooltip>
        <Button uniqueID="NewDocumentButton" row="1" column="1">
            <Graphic sharedGraphic="General_New_Document"/>
            <Command command="newDocument"/>
        </Button>
        <Button uniqueID="OpenDocumentButton" row="1" column="2">
            <Graphic sharedGraphic="General_Open_Document"/>
            <Command command="openDocument"/>
        </Button>
        <Button uniqueID="SaveDocumentButton" row="1" column="3">
            <Graphic sharedGraphic="General_Save_Document"/>
            <Command command="saveDocument"/>
        </Button>
        <Button uniqueID="PrintDocumentButton" row="1" column="4">
            <Graphic sharedGraphic="General_Print_Document"/>
            <Command command="print"/>
        </Button>
        <Button uniqueID="ShowHelpButton" row="1" column="5">
            <Graphic sharedGraphic="General_Help"/>
            <Command command="showHelpContents"/>
        </Button>
        <Button uniqueID="UndoButton" row="1" column="6">
            <Graphic sharedGraphic="General_Undo"/>
            <Command command="undo"/>
        </Button>
        <Button uniqueID="RedoButton" row="1" column="7">
            <Graphic sharedGraphic="General_Redo"/>
            <Command command="redo"/>
        </Button>
        <Button uniqueID="CutButton" row="1" column="8">
            <Graphic sharedGraphic="General_Cut"/>
            <Command command="cut"/>
        </Button>
        <Button uniqueID="CopyButton" row="1" column="9">
            <Graphic sharedGraphic="General_Copy"/>
            <Command command="copy"/>
        </Button>
        <Button uniqueID="PasteButton" row="1" column="10">
            <Graphic sharedGraphic="General_Paste"/>
            <Command command="paste"/>
        </Button>
        <Button uniqueID="ZoomMenu" buttonStyle="menu" row="1" column="11" width="3"
            buttonStyleVertical="push" heightVertical="1" widthVertical="1">
            <Title localizationName="ZoomMenuTitleID" textSource="currentZoom"/>
            <Graphic sharedGraphic="General_Choose_Zoom"/>
            <Command command="chooseZoom"/>
            <Menu sharedMenu="ZoomMenuID" />
        </Button>
        <Button uniqueID="ZoomInButton" row="1" column="14" rowVertical="12">
            <Graphic sharedGraphic="General_Zoom_In"/>
            <Command command="zoomIn"/>
        </Button>
        <Button uniqueID="ZoomOutButton" row="1" column="15" rowVertical="13">
            <Graphic sharedGraphic="General_Zoom_Out"/>
            <Command command="zoomOut"/>
        </Button>
        <Separator uniqueID="Separator1" column="5" row="1"/>
        <Separator uniqueID="Separator2" column="11" row="1"/>
    </Toolbar>
</ToolbarGroup>
