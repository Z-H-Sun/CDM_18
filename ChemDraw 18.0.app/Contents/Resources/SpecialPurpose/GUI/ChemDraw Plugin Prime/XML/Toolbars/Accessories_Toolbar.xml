<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="AccessoriesToolBarID" preferenceKey="View" columns="6" rows="1"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="AccessoriesToolbarTitle">Accessories</Title>
        <TextMenuItem>
            <Title localizationName="AccessoriesToolbarMenuID">Accessories</Title>
        </TextMenuItem>
        <Tooltip localizationName="AccessoriesToolbarTooltip">Accessories</Tooltip>
        <Button uniqueID="ViewPreferencesButton" row="1" column="1">
            <Graphic sharedGraphic="View_Preferences"/>
            <Command command="showApplicationPreferences"/>
        </Button>
        <Button uniqueID="ViewDocSettingsButton" row="1" column="2">
            <Graphic sharedGraphic="Document_Settings"/>
            <Command command="showDocumentSettings"/>
        </Button>
        <Button uniqueID="ViewRxnInterpretationButton" row="1" column="3">
            <Graphic sharedGraphic="View_RxnInterpretation"/>
            <Command command="toggleReactionInterpretation"/>
        </Button>
        <Button uniqueID="ViewPeriodicTableButton" row="1" column="4">
            <Graphic sharedGraphic="View_PeriodicTable"/>
            <Command command="togglePeriodicTableWindow"/>
        </Button>
        <Button uniqueID="ViewCharacterMapButton" row="1" column="5">
            <Graphic sharedGraphic="View_CharacterMap"/>
            <Command command="toggleCharacterMapWindow"/>
        </Button>
        <Button uniqueID="ViewAnalysisButton" row="1" column="6">
            <Graphic sharedGraphic="View_Analysis"/>
            <Command command="toggleAnalysisWindow"/>
        </Button>
     </Toolbar>
</ToolbarGroup>
