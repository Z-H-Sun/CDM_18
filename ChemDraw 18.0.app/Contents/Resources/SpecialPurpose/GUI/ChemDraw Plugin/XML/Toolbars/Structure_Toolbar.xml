<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="StructureToolBarID" preferenceKey="Structure" columns="9" rows="1"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true">
        <Title localizationName="StructureToolbarTitle">Structure</Title>
        <TextMenuItem>
            <Title localizationName="StructureToolbarMenuID">Show Structure Toolbar</Title>
        </TextMenuItem>
        <Tooltip localizationName="StructureToolbarTooltip">Structure commands</Tooltip>
        <Button uniqueID="NameToStructureButton" row="1" column="1">
            <Graphic sharedGraphic="Structure_NameToStruct"/>
            <Command command="convertNameToStructure"/>
        </Button>
        <Button uniqueID="StructureToNameButton" row="1" column="2">
            <Graphic sharedGraphic="Structure_StructToName"/>
            <Command command="convertStructureToName"/>
        </Button>
        <Button uniqueID="StoichiometryButton" row="1" column="3">
            <Graphic sharedGraphic="Structure_StoichiometryGrid"/>
            <Command command="analyzeStoichiometry"/>
        </Button>
         <Button uniqueID="CleanStructureButton" row="1" column="4">
            <Graphic sharedGraphic="General_Clean_Up_Structure"/>
            <Command command="cleanStructure"/>
        </Button>
        <Button uniqueID="CleanReactionButton" row="1" column="5">
            <Graphic sharedGraphic="General_Clean_Up_Reaction"/>
            <Command command="cleanReaction"/>
        </Button>
         <Button uniqueID="CleanSequenceButton" row="1" column="6">
            <Graphic sharedGraphic="General_Clean_Up_Sequence"/>
            <Command command="cleanSequence"/>
        </Button>
       <Button uniqueID="CheckStructureButton" row="1" column="7">
            <Graphic sharedGraphic="General_Check_Structure"/>
            <Command command="checkStructure"/>
        </Button>
        <Button uniqueID="ExpandLabelButton" row="1" column="8">
            <Graphic sharedGraphic="Expand_Label"/>
            <Command command="expandLabel"/>
        </Button>
        <Button uniqueID="ContractLabelButton" row="1" column="9">
            <Graphic sharedGraphic="Contract_Label"/>
            <Command command="contractLabel"/>
        </Button>
   </Toolbar>
</ToolbarGroup>
