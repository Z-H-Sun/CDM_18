<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="FragmentationToolBarID" preferenceKey="Fragmentation" columns="2" rows="2"
        defaultOrientation="horizontal"
        hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="FragmentationToolbarTitle">Fragmentation</Title>
        <TextMenuItem>
            <Title localizationName="FragmentationToolbarMenuItem">Fragmentation Tools</Title>
        </TextMenuItem>
        <Tooltip localizationName="FragmentationToolbarToolTip">Fragmentation Tools</Tooltip>
        <Button buttonStyle="toggle" column="1" row="1" titleMode="hidden" uniqueID="FragmentationButtonID">
            <Graphic sharedGraphic="Fragmentation_MassFrag"/>
            <Command command="chooseFragmentTool_MassFragmentation"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="1" titleMode="hidden" uniqueID="SynthesisButtonID">
            <Graphic sharedGraphic="Fragmentation_Synthesis"/>
            <Command command="chooseFragmentTool_Synthesis"/>
        </Button>
        <Button buttonStyle="toggle" column="1" row="2" titleMode="hidden" uniqueID="RetroSynthButtonID">
            <Graphic sharedGraphic="Fragmentation_RetroSynth"/>
            <Command command="chooseFragmentTool_Retrosynthesis"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="2" titleMode="hidden" uniqueID="EnhancedRetroSynthButtonID">
            <Graphic sharedGraphic="Fragmentation_EnhancedRetroSynth"/>
            <Command command="chooseFragmentTool_EnhancedRetrosynthesis"/>
        </Button>
   </Toolbar>
</ToolbarGroup>
