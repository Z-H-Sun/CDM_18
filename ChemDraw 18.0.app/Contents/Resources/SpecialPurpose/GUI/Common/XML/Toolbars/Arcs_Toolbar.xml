<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="ArcsToolBarID" preferenceKey="Arc Tools" columns="4" rows="2"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="ArcsToolbarTitle">Arcs</Title>
        <TextMenuItem>
            <Title localizationName="ArcsToolbarMenuItem">Arcs</Title>
        </TextMenuItem>
        <Tooltip localizationName="ArcsToolbarToolTip">Arcs</Tooltip>
        <Button buttonStyle="toggle" row="1" column="1" titleMode="hidden" uniqueID="DrawingButtonID_Arc_270">
            <Graphic sharedGraphic="Draw_Arc_270"/>
            <Command command="chooseDrawingTool_Arc_270"/>
        </Button>
        <Button buttonStyle="toggle" row="1" column="2" titleMode="hidden" uniqueID="DrawingButtonID_Arc_180">
            <Graphic sharedGraphic="Draw_Arc_180"/>
            <Command command="chooseDrawingTool_Arc_180"/>
        </Button>
        <Button buttonStyle="toggle" row="1" column="3" titleMode="hidden" uniqueID="DrawingButtonID_Arc_120">
            <Graphic sharedGraphic="Draw_Arc_120"/>
            <Command command="chooseDrawingTool_Arc_120"/>
        </Button>
        <Button buttonStyle="toggle" row="1" column="4" titleMode="hidden" uniqueID="DrawingButtonID_Arc_90">
            <Graphic sharedGraphic="Draw_Arc_90"/>
            <Command command="chooseDrawingTool_Arc_90"/>
        </Button>
        <Button buttonStyle="toggle" row="2" column="1" titleMode="hidden" uniqueID="DrawingButtonID_Arc_270_Dashed">
            <Graphic sharedGraphic="Draw_Arc_270_Dashed"/>
            <Command command="chooseDrawingTool_Arc_270_Dashed"/>
        </Button>
        <Button buttonStyle="toggle" row="2" column="2" titleMode="hidden" uniqueID="DrawingButtonID_Arc_180_Dashed">
            <Graphic sharedGraphic="Draw_Arc_180_Dashed"/>
            <Command command="chooseDrawingTool_Arc_180_Dashed"/>
        </Button>
        <Button buttonStyle="toggle" row="2" column="3" titleMode="hidden" uniqueID="DrawingButtonID_Arc_120_Dashed">
            <Graphic sharedGraphic="Draw_Arc_120_Dashed"/>
            <Command command="chooseDrawingTool_Arc_120_Dashed"/>
        </Button>
        <Button buttonStyle="toggle" row="2" column="4" titleMode="hidden" uniqueID="DrawingButtonID_Arc_90_Dashed">
            <Graphic sharedGraphic="Draw_Arc_90_Dashed"/>
            <Command command="chooseDrawingTool_Arc_90_Dashed"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
