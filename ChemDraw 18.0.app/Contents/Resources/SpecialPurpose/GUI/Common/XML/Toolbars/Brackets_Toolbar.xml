<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="BracketToolBarID" preferenceKey="Brackets" columns="4" rows="2"
        defaultOrientation="horizontal"
        hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="BracketToolbarTitle">Brackets</Title>
        <TextMenuItem>
            <Title localizationName="BracketToolbarMenuItem">Brackets</Title>
        </TextMenuItem>
        <Tooltip localizationName="BracketToolbarToolTip">Brackets</Tooltip>
        <Button buttonStyle="toggle" column="1" row="1" titleMode="hidden" uniqueID="SquareBracketButtonID">
            <Graphic sharedGraphic="Bracket_SquareBrackets"/>
            <Command command="chooseBracketTool_SquareBrackets"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="1" titleMode="hidden" uniqueID="ParenthesisButtonID">
            <Graphic sharedGraphic="Bracket_Parenthesis"/>
            <Command command="chooseBracketTool_Parenthesis"/>
        </Button>
        <Button buttonStyle="toggle" column="3" row="1" titleMode="hidden" uniqueID="CurlyBracketsButtonID">
            <Graphic sharedGraphic="Bracket_CurlyBrackets"/>
            <Command command="chooseBracketTool_CurlyBrackets"/>
        </Button>
        <Button buttonStyle="toggle" column="4" row="1" titleMode="hidden" uniqueID="DoubleDaggerButtonID">
            <Graphic sharedGraphic="Bracket_DoubleDagger"/>
            <Command command="chooseBracketTool_DoubleDagger"/>
        </Button>
        <Button buttonStyle="toggle" column="1" row="2" titleMode="hidden" uniqueID="LeftSquareBracketButtonID">
            <Graphic sharedGraphic="Bracket_LeftSquareBracket"/>
            <Command command="chooseBracketTool_LeftSquareBracket"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="2" titleMode="hidden" uniqueID="LeftParenthesisButtonID">
            <Graphic sharedGraphic="Bracket_LeftParenthesis"/>
            <Command command="chooseBracketTool_LeftParenthesis"/>
        </Button>
        <Button buttonStyle="toggle" column="3" row="2" titleMode="hidden" uniqueID="LeftCurlyBracketButtonID">
            <Graphic sharedGraphic="Bracket_LeftCurlyBracket"/>
            <Command command="chooseBracketTool_LeftCurlyBracket"/>
        </Button>
        <Button buttonStyle="toggle" column="4" row="2" titleMode="hidden" uniqueID="SingleDaggerButtonID">
            <Graphic sharedGraphic="Bracket_SingleDagger"/>
            <Command command="chooseBracketTool_SingleDagger"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
