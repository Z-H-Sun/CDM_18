<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="ChainToolBarID" preferenceKey="Chain" columns="2" rows="1" 
    		defaultOrientation="horizontal" 
    		hideOnApplicationSwitch="true" 
    		menuLocation="otherMenu">
        <Title localizationName="ChainToolbarTitle">Chains</Title>
        <TextMenuItem>
            <Title localizationName="ChainToolbarMenuItem">Chains</Title>
        </TextMenuItem>
        <Tooltip localizationName="ChainToolbarToolTip">Chains</Tooltip>
        <Button buttonStyle="toggle" column="1" row="1" titleMode="hidden" uniqueID="AcyclicChain_ButtonID">
            <Graphic sharedGraphic="Main_Acyclic_Chain"/>
            <Command command="chooseAlkaneTool_Acyclic_Chain"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="1" titleMode="hidden" uniqueID="SnakingChain_ButtonID">
            <Graphic sharedGraphic="Main_Snaking_Chain"/>
            <Command command="chooseAlkaneTool_Snaking_Chain"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
