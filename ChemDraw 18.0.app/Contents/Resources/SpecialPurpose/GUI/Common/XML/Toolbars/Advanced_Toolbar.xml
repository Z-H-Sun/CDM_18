<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="ChromatographyToolBarID" preferenceKey="Chromatography" columns="2" rows="1"
        defaultOrientation="horizontal"
        hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="ChromatographyToolbarTitle">Chromatography</Title>
        <TextMenuItem>
            <Title localizationName="ChromatographyToolbarMenuItem">Chromatography Tools</Title>
        </TextMenuItem>
        <Tooltip localizationName="ChromatographyToolbarToolTip">Chromatography Tools</Tooltip>
        <Button buttonStyle="toggle" column="1" row="1" titleMode="hidden" uniqueID="TLC_Plate_ButtonID">
            <Graphic sharedGraphic="TLCPlateTool"/>
            <Command command="chooseTLCPlateTool"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="1" titleMode="hidden" uniqueID="GEP_Plate_ButtonID">
            <Command command="chooseGEPPlateTool"/>
            <Graphic sharedGraphic="GEPPlateTool"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
