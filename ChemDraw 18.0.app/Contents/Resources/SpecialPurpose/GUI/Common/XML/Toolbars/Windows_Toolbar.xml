<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="WindowsToolBarID" preferenceKey="Windows" columns="3" rows="1"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="WindowsToolbarTitle">Windows</Title>
        <TextMenuItem>
            <Title localizationName="WindowsToolbarMenuID">WIndows</Title>
        </TextMenuItem>
        <Tooltip localizationName="WindowsToolbarTooltip">Windows commands</Tooltip>
        <Button uniqueID="WindowsCascadeButton" row="1" column="1">
            <Graphic sharedGraphic="Windows_Cascade"/>
            <Command command="window_Cascade"/>
        </Button>
        <Button uniqueID="WindowsTileHorizButton" row="1" column="2">
            <Graphic sharedGraphic="Windows_TileHoriz"/>
            <Command command="window_TileHorizontal"/>
        </Button>
        <Button uniqueID="WindowsTileVertButton" row="1" column="3">
            <Graphic sharedGraphic="Windows_TileVert"/>
            <Command command="window_TileVertical"/>
        </Button>
     </Toolbar>
</ToolbarGroup>
