<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="ObjectToolBarID" preferenceKey="Object" columns="10" rows="2"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true">
        <Title localizationName="ObjectToolbarTitle">Object</Title>
        <TextMenuItem>
            <Title localizationName="ObjectToolbarMenuItem">Show Object Toolbar</Title>
        </TextMenuItem>
        <Tooltip localizationName="ObjectToolbarToolTip">Object Ordering and Alignment</Tooltip>
        <Button buttonStyle="push" column="1" row="1" titleMode="hidden" uniqueID="GroupButtonID">
            <Graphic sharedGraphic="Object_Group"/>
            <Command command="group"/>
        </Button>
        <Button buttonStyle="push" column="2" row="1" titleMode="hidden" uniqueID="UnGroupButtonID">
            <Graphic sharedGraphic="Object_GroupDelete"/>
            <Command command="ungroup"/>
        </Button>
        <Button buttonStyle="push" column="3" row="1" titleMode="hidden" uniqueID="GroupIntegralButtonID">
            <Graphic sharedGraphic="Object_GroupIntegral"/>
            <Command command="groupIntegral"/>
        </Button>
        <Button buttonStyle="push" column="4" row="1" titleMode="hidden" uniqueID="BringForwardButtonID">
            <Graphic sharedGraphic="Object_SendToFront"/>
            <Command command="bringToFront"/>
        </Button>

        <Button buttonStyle="push" column="5" row="1" titleMode="hidden" uniqueID="SendBackButtonID">
            <Graphic sharedGraphic="Object_SendToBack"/>
            <Command command="sendToBack"/>
        </Button>
        <Button buttonStyle="push" column="6" row="1" titleMode="hidden" uniqueID="RotateHorizButtonID">
            <Graphic sharedGraphic="Object_RotateHoriz"/>
            <Command command="rotateObjects180Horizontal"/>
        </Button>
        <Button buttonStyle="push" column="7" row="1" titleMode="hidden" uniqueID="RotateVertButtonID">
            <Graphic sharedGraphic="Object_RotateVert"/>
            <Command command="rotateObjects180Vertical"/>
        </Button>
        <Button buttonStyle="push" column="8" row="1" titleMode="hidden" uniqueID="FlipHorizButtonID">
            <Graphic sharedGraphic="Object_FlipHoriz"/>
            <Command command="flipObjectsHorizontal"/>
        </Button>
        <Button buttonStyle="push" column="9" row="1" titleMode="hidden" uniqueID="FlipVertButtonID">
            <Graphic sharedGraphic="Object_FlipVert"/>
            <Command command="flipObjectsVertical"/>
        </Button>
        <Button buttonStyle="push" column="10" row="1" titleMode="hidden" uniqueID="CenterOnPageButtonID">
            <Graphic sharedGraphic="Object_Center"/>
            <Command command="centerObjectOnPage"/>
        </Button>
        <Button buttonStyle="push" column="1" row="2" titleMode="hidden" uniqueID="ObjectSettingsButtonID">
            <Graphic sharedGraphic="Object_Settings"/>
            <Command command="showObjectSettingsDialog"/>
        </Button>
        <Button buttonStyle="push" column="2" row="2" titleMode="hidden" uniqueID="AnnotateButtonID">
            <Graphic sharedGraphic="Object_Annotate"/>
            <Command command="showAnnotationDialog"/>
        </Button>
        <Button buttonStyle="push" column="3" row="2" titleMode="hidden" uniqueID="AlignLeftButtonID">
            <Graphic sharedGraphic="Objects_AlignLeft"/>
            <Command command="alignLeftEdges"/>
        </Button>
        <Button buttonStyle="push" column="4" row="2" titleMode="hidden" uniqueID="AlignRightButtonID">
            <Graphic sharedGraphic="Objects_AlignRight"/>
            <Command command="alignRightEdges"/>
        </Button>
        <Button buttonStyle="push" column="5" row="2" titleMode="hidden" uniqueID="AlignTopButtonID">
            <Graphic sharedGraphic="Objects_AlignTop"/>
            <Command command="alignTopEdges"/>
        </Button>
        <Button buttonStyle="push" column="6" row="2" titleMode="hidden" uniqueID="AlignBottomButtonID">
            <Graphic sharedGraphic="Objects_AlignBottom"/>
            <Command command="alignBottomEdges"/>
        </Button>
        <Button buttonStyle="push" column="7" row="2" titleMode="hidden" uniqueID="AlignLeftRightCenterButtonID">
            <Graphic sharedGraphic="Objects_AlignVertCenters"/>
            <Command command="alignLeftRightCenters"/>
        </Button>
        <Button buttonStyle="push" column="8" row="2" titleMode="hidden" uniqueID="AlignTopBottomCentersButtonID">
            <Graphic sharedGraphic="Objects_AlignHorizCenters"/>
            <Command command="alignTopBottomCenters"/>
        </Button>
        <Button buttonStyle="push" column="9" row="2" titleMode="hidden" uniqueID="DistributeHorizButtonID">
            <Graphic sharedGraphic="Object_DistributeHoriz"/>
            <Command command="distributeObjectsHorizontally"/>
        </Button>
        <Button buttonStyle="push" column="10" row="2" titleMode="hidden" uniqueID="DistributeVertButtonID">
            <Graphic sharedGraphic="Object_DistributeVert"/>
            <Command command="distributeObjectsVertically"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
