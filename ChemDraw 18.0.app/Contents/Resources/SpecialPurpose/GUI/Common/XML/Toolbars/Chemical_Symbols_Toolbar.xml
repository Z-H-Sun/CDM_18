<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="ChemicalSymbolsToolBarID" preferenceKey="Symbols" columns="8"
        rows="2" defaultOrientation="horizontal" hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="ChemicalSymbolsToolbarTitle">Chemical Symbols</Title>
        <TextMenuItem>
            <Title localizationName="ChemicalSymbolsToolbarMenuItem">Chemical Symbols</Title>
        </TextMenuItem>
        <Tooltip localizationName="ChemicalSymbolsToolbarToolTip">Chemical Symbols Tools</Tooltip>
        <Button buttonStyle="toggle" column="1" row="1" titleMode="hidden" uniqueID="ButtonID_CirclePlus">
            <Graphic sharedGraphic="Symbol_CirclePlus"/>
            <Command command="chooseSymbolTool_CirclePlus"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="1" titleMode="hidden" uniqueID="ButtonID_Plus">
            <Graphic sharedGraphic="Symbol_Plus"/>
            <Command command="chooseSymbolTool_Plus"/>
        </Button>
        <Button buttonStyle="toggle" column="3" row="1" titleMode="hidden" uniqueID="ButtonID_RadicalCation">
            <Graphic sharedGraphic="Symbol_RadicalCation"/>
            <Command command="chooseSymbolTool_RadicalCation"/>
        </Button>
        <Button buttonStyle="toggle" column="4" row="1" titleMode="hidden" uniqueID="ButtonID_DoubleRadical">
            <Graphic sharedGraphic="Symbol_DoubleRadical"/>
            <Command command="chooseSymbolTool_DoubleRadical"/>
        </Button>
		<Button buttonStyle="toggle" column="5" row="1" titleMode="hidden" uniqueID="ElectronPushingButtonID">
			<Graphic sharedGraphic="EPT_LonePair"/>
			<Command command="choosePushElectronTool"/>
		</Button>
        <Button buttonStyle="toggle" column="6" row="1" titleMode="hidden" uniqueID="ButtonID_HDot">
            <Graphic sharedGraphic="Symbol_HDot"/>
            <Command command="chooseSymbolTool_HDot"/>
        </Button>
        <Button buttonStyle="toggle" column="7" row="1" titleMode="hidden" uniqueID="ButtonID_Attachment_Diamond">
            <Graphic sharedGraphic="Symbol_Attachment_Diamond"/>
            <Command command="chooseSymbolTool_Attachment_Diamond"/>
        </Button>
        <Button buttonStyle="toggle" column="8" row="1" titleMode="hidden" uniqueID="ButtonID_PolymerBeadAttachmentPoint">
            <Graphic sharedGraphic="Symbol_PolyBeadAttachPoint"/>
            <Command command="chooseSymbolTool_PolymerBeadAttachmentPoint"/>
        </Button>
        <Button buttonStyle="toggle" column="1" row="2" titleMode="hidden" uniqueID="ButtonID_CircleMinus">
            <Graphic sharedGraphic="Symbol_CircleMinus"/>
            <Command command="chooseSymbolTool_CircleMinus"/>
        </Button>
        <Button buttonStyle="toggle" column="2" row="2" titleMode="hidden" uniqueID="ButtonID_Minus">
            <Graphic sharedGraphic="Symbol_Minus"/>
            <Command command="chooseSymbolTool_Minus"/>
        </Button>
        <Button buttonStyle="toggle" column="3" row="2" titleMode="hidden" uniqueID="ButtonID_RadicalAnion">
            <Graphic sharedGraphic="Symbol_RadicalAnion"/>
            <Command command="chooseSymbolTool_RadicalAnion"/>
        </Button>
        <Button buttonStyle="toggle" column="4" row="2" titleMode="hidden" uniqueID="ButtonID_Radical">
            <Graphic sharedGraphic="Symbol_Radical"/>
            <Command command="chooseSymbolTool_Radical"/>
        </Button>
		<Button buttonStyle="toggle" column="5" row="2" titleMode="hidden" uniqueID="RadicalPushingButtonID">
			<Graphic sharedGraphic="EPT_Radical"/>
			<Command command="choosePushRadicalTool"/>
		</Button>
        <Button buttonStyle="toggle" column="6" row="2" titleMode="hidden" uniqueID="ButtonID_HDash">
            <Graphic sharedGraphic="Symbol_HDash"/>
            <Command command="chooseSymbolTool_HDash"/>
        </Button>
        <Button buttonStyle="toggle" column="7" row="2" titleMode="hidden" uniqueID="ButtonID_Attachment_Star">
            <Graphic sharedGraphic="Symbol_Attachment_Star"/>
            <Command command="chooseSymbolTool_Attachment_Star"/>
        </Button>
        <Button buttonStyle="toggle" column="8" row="2" titleMode="hidden" uniqueID="ButtonID_Attachment_Wavy">
            <Graphic sharedGraphic="Symbol_Attachment_Wavy"/>
            <Command command="chooseSymbolTool_Attachment_Wavy"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
