<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="CurvesToolBarID" preferenceKey="Curves" columns="8" rows="1"
        defaultOrientation="horizontal" hideOnApplicationSwitch="true" menuLocation="otherMenu">
        <Title localizationName="CurvesToolbarTitle">Curve Styles</Title>
        <TextMenuItem>
            <Title localizationName="CurvesToolbarMenuID">Curves</Title>
        </TextMenuItem>
        <Tooltip localizationName="CurvesToolbarTooltip">Curve Styles</Tooltip>
        <Button uniqueID="PlainCurveButton" row="1" column="1">
            <Graphic sharedGraphic="Curve_Plain"/>
            <Command command="curveStyle_Plain"/>
        </Button>
        <Button uniqueID="ClosedCurveButton" row="1" column="2">
            <Graphic sharedGraphic="Curve_Closed"/>
            <Command command="curveStyle_Closed"/>
        </Button>
        <Button uniqueID="DashedCurveButton" row="1" column="3">
            <Graphic sharedGraphic="Curve_Dashed"/>
            <Command command="curveStyle_Dashed"/>
        </Button>
        <Button uniqueID="BoldCurveButton" row="1" column="4">
            <Graphic sharedGraphic="Curve_Bold"/>
            <Command command="curveStyle_Bold"/>
        </Button>
        <Button uniqueID="DoubledCurveButton" row="1" column="5">
            <Graphic sharedGraphic="Curve_Doubled"/>
            <Command command="curveStyle_Doubled"/>
        </Button>
        <Button uniqueID="FadedCurveButton" row="1" column="6">
            <Graphic sharedGraphic="Curve_Faded"/>
            <Command command="curveStyle_Faded"/>
        </Button>
        <Button uniqueID="ShadedCurveButton" row="1" column="7">
            <Graphic sharedGraphic="Curve_Shaded"/>
            <Command command="curveStyle_Shaded"/>
        </Button>
        <Button uniqueID="FilledCurveButton" row="1" column="8">
            <Graphic sharedGraphic="Curve_Filled"/>
            <Command command="curveStyle_Filled"/>
        </Button>
    </Toolbar>
</ToolbarGroup>
