        <Button row="2" column="6" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_Micelle"/>
            <Command command="chooseBioTool_Membrane_Micelle"/>
        </Button>
        <Button row="2" column="7" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_MembraneEllipse"/>
            <Command command="chooseBioTool_Membrane_Ellipse"/>
        </Button>
        <Button row="2" column="8" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_EndoplasmicReticulum"/>
            <Command command="chooseBioTool_Er"/>
        </Button>
        <Button row="2" column="9" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_Mitocondrion"/>
