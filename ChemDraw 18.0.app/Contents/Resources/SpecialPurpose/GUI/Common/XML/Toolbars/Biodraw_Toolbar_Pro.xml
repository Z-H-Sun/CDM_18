        </Button>
        <Button row="1" column="10" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_RibosomeA"/>
            <Command command="chooseBioTool_RibosomeA"/>
        </Button>
        <Button row="2" column="1" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_G_Protein_AlphaSubunit"/>
            <Command command="chooseBioTool_GProteinAlpha"/>
