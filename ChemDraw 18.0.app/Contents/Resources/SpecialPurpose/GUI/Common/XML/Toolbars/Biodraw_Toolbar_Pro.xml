<?xml version="1.0" encoding="UTF-8"?>
<ToolbarGroup xmlns="http://www.cambridgesoft.com/software/ChemDraw/gui"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cambridgesoft.com/software/ChemDraw/gui ToolBar.xsd">
    <Toolbar uniqueID="BioDrawToolBarID_PRO" preferenceKey="BioDraw" columns="10" rows="2"
       defaultOrientation="horizontal" hideOnApplicationSwitch="true" >
        <Title localizationName="BioDrawToolbarTitle">BioDraw</Title>
        <TextMenuItem>
            <Title localizationName="BioDrawToolbarMenuItem">Show BioDraw Toolbar</Title>
        </TextMenuItem>
        <Tooltip localizationName="BioDrawToolbarToolTip">BioDraw Ordering and Alignment</Tooltip>
        <Button row="1" column="1" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_1_Substrate_Enzyme"/>
            <Command command="chooseBioTool_1SubstrateEnzyme"/>
        </Button>
        <Button row="1" column="2" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_2_Substrate_Enzyme"/>
            <Command command="chooseBioTool_2SubstrateEnzyme"/>
        </Button>
        <Button row="1" column="3" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_Receptor"/>
            <Command command="chooseBioTool_Receptor"/>
        </Button>
        <Button row="1" column="4" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_IonChannel"/>
            <Command command="chooseBioTool_IonChannel"/>
        </Button>
        <Button row="1" column="5" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_HelixProtein"/>
            <Command command="chooseBioTool_HelixProtein"/>
        </Button>
        <Button row="1" column="6" buttonStyle="toggle">
            <Graphic sharedGraphic="BD_MembraneLine"/>
